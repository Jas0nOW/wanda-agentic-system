{
    "$schema": "https://opencode.ai/config.json",
    "plugin": [
        "oh-my-opencode@3.2.3",
        "opencode-orchestrator@latest",
        "opencode-antigravity-auth@1.4.3",
        "opencode-antigravity-quota@latest",
        "opencode-handoff@latest",
        "opencode-scheduler@latest",
        "opencode-websearch-cited@latest",
        "opencode-knowledge@latest",
        "opencode-supermemory@latest",
        "opencode-agent-skills@latest"
    ],
    "provider": {
        "google": {
            "name": "Google",
            "models": {
                "antigravity-gemini-3-pro": {
                    "name": "Gemini 3 Pro (Antigravity)",
                    "limit": {
                        "context": 1048576,
                        "output": 65535
                    },
                    "modalities": {
                        "input": [
                            "text",
                            "image",
                            "pdf"
                        ],
                        "output": [
                            "text"
                        ]
                    },
                    "variants": {
                        "low": {
                            "thinkingLevel": "low"
                        },
                        "high": {
                            "thinkingLevel": "high"
                        }
                    }
                },
                "antigravity-gemini-3-flash": {
                    "name": "Gemini 3 Flash (Antigravity)",
                    "limit": {
                        "context": 1048576,
                        "output": 65536
                    },
                    "modalities": {
                        "input": [
                            "text",
                            "image",
                            "pdf"
                        ],
                        "output": [
                            "text"
                        ]
                    },
                    "variants": {
                        "minimal": {
                            "thinkingLevel": "minimal"
                        },
                        "low": {
                            "thinkingLevel": "low"
                        },
                        "medium": {
                            "thinkingLevel": "medium"
                        },
                        "high": {
                            "thinkingLevel": "high"
                        }
                    }
                },
                "antigravity-claude-sonnet-4-5": {
                    "name": "Claude Sonnet 4.5 (Antigravity)",
                    "limit": {
                        "context": 200000,
                        "output": 64000
                    },
                    "modalities": {
                        "input": [
                            "text",
                            "image",
                            "pdf"
                        ],
                        "output": [
                            "text"
                        ]
                    }
                },
                "antigravity-claude-sonnet-4-5-thinking": {
                    "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
                    "limit": {
                        "context": 200000,
                        "output": 64000
                    },
                    "modalities": {
                        "input": [
                            "text",
                            "image",
                            "pdf"
                        ],
                        "output": [
                            "text"
                        ]
                    },
                    "variants": {
                        "low": {
                            "thinkingConfig": {
                                "thinkingBudget": 8192
                            }
                        },
                        "max": {
                            "thinkingConfig": {
                                "thinkingBudget": 32768
                            }
                        }
                    }
                },
                "antigravity-claude-opus-4-5-thinking": {
                    "name": "Claude Opus 4.5 Thinking (Antigravity)",
                    "limit": {
                        "context": 200000,
                        "output": 64000
                    },
                    "modalities": {
                        "input": [
                            "text",
                            "image",
                            "pdf"
                        ],
                        "output": [
                            "text"
                        ]
                    },
                    "variants": {
                        "low": {
                            "thinkingConfig": {
                                "thinkingBudget": 8192
                            }
                        },
                        "max": {
                            "thinkingConfig": {
                                "thinkingBudget": 32768
                            }
                        }
                    }
                }
            }
        }
    },
    "mcp": {
        "MCP_DOCKER": {
            "type": "local",
            "command": [
                "docker",
                "mcp",
                "gateway",
                "run"
            ],
            "enabled": true
        },
        "hostinger-web": {
            "type": "local",
            "command": [
                "bash",
                "-c",
                "API_TOKEN={{HOSTINGER_WEB_API_TOKEN}} npx -y hostinger-api-mcp"
            ],
            "enabled": true
        },
        "hostinger-vps": {
            "type": "local",
            "command": [
                "bash",
                "-c",
                "API_TOKEN={{HOSTINGER_VPS_API_TOKEN}} npx -y hostinger-api-mcp"
            ],
            "enabled": true
        }
    }
}
