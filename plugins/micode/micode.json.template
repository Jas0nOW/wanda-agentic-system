{
  "$schema": "https://micode.dev/schema.json",
  "version": "1.0.4",
  
  "_comment": "WANDA MiCode Configuration - Jas0n's Setup",
  
  "agents": {
    "brainstormer": {
      "model": "{{BRAINSTORMER_MODEL}}",
      "temperature": 0.8
    },
    "commander": {
      "disable": true
    },
    "planner": {
      "disable": true
    }
  },
  
  "features": {
    "mindmodelInjection": true
  },
  
  "compactionThreshold": 0.5,
  
  "fragments": {
    "brainstormer": ["prompts/fragments/brainstorm.md"]
  }
}
